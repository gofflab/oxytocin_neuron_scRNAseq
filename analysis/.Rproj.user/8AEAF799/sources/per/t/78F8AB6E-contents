# FM_clean <- as.matrix(FM) - beta %*% t(X.model_mat[, -1])

FM_clean<-removeBatchEffect(FM,covariate=projectionData$MMcyan,design=matrix(1,ncol(FM),1))

#FM_clean[projectionData$celltype==1 & projectionData$Day=="Day-"]

